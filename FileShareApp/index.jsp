<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>File Sharing Home</title>
    <style>
        body { font-family: Arial; margin: 100px; text-align: center; background-color: #f9f9f9; }
        .container { display: inline-block; padding: 50px; border: 1px solid #ddd; border-radius: 8px; background: white; box-shadow: 0 0 10px rgba(0,0,0,0.1); }
        h1 { margin-bottom: 50px; color: #333; }
        button { padding: 15px 40px; font-size: 18px; margin: 20px; cursor: pointer; border: none; border-radius: 6px; color: white; background-color: #007BFF; transition: 0.3s; }
        button:hover { background-color: #0056b3; }
        a { text-decoration: none; }
    </style>
</head>
<body>
<div class="container">
    <h1>Welcome to File Sharing</h1>
    <a href="upload.jsp"><button>Upload File</button></a>
    <a href="access.jsp"><button>Access File</button></a>
</div>
</body>
</html>
